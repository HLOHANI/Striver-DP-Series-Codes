int mod = (int)(1e9 + 7);
int uniPath(int i, int j, vector< vector< int> > &mat)
{
	if(i>=0 && j>=0 && mat[i][j]==-1) return 0;
	if(i==0 && j==0) return 1;
	if(i<0 || j<0 ) return 0;
	
	int up=uniPath(i-1,j,mat);
	int left=uniPath(i,j-1,mat);
	
	return (up+left)%mod;
}
int mazeObstacles(int n, int m, vector< vector< int> > &mat) {
    // Write your code here
	return uniPath(n-1,m-1,mat);
}
